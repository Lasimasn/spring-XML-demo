<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="Movie1" class="com.stackroute.domain.beans.Movie" autowire="byName">

    </bean>
    <bean id="Movie2" class="com.stackroute.domain.beans.Movie">
        <constructor-arg name="actor1" ref="actor1" />
        <constructor-arg name="actor2" ref="actor2" />
        <constructor-arg name="actor3" ref="actor3" />
    </bean>
    <bean id="actor1" class="com.stackroute.domain.beans.Actor">
        <property name="name" value="Vicky Kaushal"/>
        <property name="gender" value="Male" />
        <property name="age" value="34" />
    </bean>
    <bean id="actor2" class="com.stackroute.domain.beans.Actor">
        <property name="name" value="Alia Bhatt"/>
        <property name="gender" value="Female" />
        <property name="age" value="26" />
    </bean>
    <bean id="actor3" class="com.stackroute.domain.beans.Actor">
        <property name="name" value="Karthik Aryan"/>
        <property name="gender" value="Male" />
        <property name="age" value="28" />
    </bean>

    <bean id="lifecycle" class="com.stackroute.domain.beans.LifeCycle" init-method="customInit" destroy-method="customDestroy">

    </bean>
    </beans>
